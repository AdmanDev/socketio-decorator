{
  "name": "@admandev/socketio-decorator",
  "version": "1.3.0",
  "description": "This library allows you to use Socket.io with TypeScript decorators, simplifying the integration and usage of Socket.io in a TypeScript environment.",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "author": "Admandev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdmanDev/socketio-decorator"
  },
  "keywords": [
    "socket.io",
    "decorator",
    "typescript",
    "nodejs"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --sourcemap",
    "test": "jest",
    "lint": "eslint src",
    "upgrade-deps": "ncu --upgrade && yarn install"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "8.36.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "ncu": "^0.2.1",
    "shelljs": "^0.8.5",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "socket.io": "^4.8.1"
  },
  "optionalDependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2"
  }
}
